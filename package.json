{
  "name": "@thiagoelg/node-printer",
  "description": "Electron.js printer bindings",
  "version": "0.6.2",
  "homepage": "https://github.com/thiagoelg/node-printer",
  "author": {
    "name": "Ion Lupascu",
    "url": "http://program-support.co.uk/",
    "email": "ionlupascu@gmail.com"
  },
  "contributors": [
    "Thiago Lugli <thiagoelg@gmail.com>",
    "Eko Eryanto <ekoeryanto@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/thiagoelg/node-printer.git"
  },
  "devDependencies": {
    "nodeunit": "*",
    "prebuild": "^11.0.3",
    "prebuild-ci": "^4.0.0",
    "node-abi": "^3.24.0",
    "patch-package": "^6.4.7"
  },
  "engines": {
    "electron": ">=28.1.3"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "apply-patches": "patch-package",
    "prebuild-all": "prebuild --all --force --strip --verbose --openssl_fips=X",
    "prebuild": "prebuild",
    "rebuild":"node-gyp rebuild",
    "electron-rebuild": "node-gyp rebuild --target_platform=win32 --target_arch=x64  --runtime=electron --target=30.0.0 --dist-url=https://electronjs.org/headers",
    "test": "nodeunit test"
  },
  "binary": {
    "module_name": "node_printer",
    "module_path": "./lib/",
    "host": "https://github.com/thiagoelg/node-printer/releases/download/",
    "remote_path": "v{version}"
  },
  "licenses": [
    {
      "type": "BSD"
    }
  ],
  "main": "./lib/printer",
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.11",
    "nan": "^2.18.0",
    "node-pre-gyp": "*"
  },
  "types": "types/index.d.ts"
}
